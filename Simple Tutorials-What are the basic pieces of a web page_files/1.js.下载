    };
})();

if (!Array.prototype.indexOf) {
    Array.prototype.indexOf = function(t) {
        "use strict";
        if (this == null) {
            throw new TypeError();
        }
        var o = Object(this);
        var e = o.length >>> 0;
        if (e === 0) {
            return -1;
        }
        var i = 0;
        if (arguments.length > 1) {
            i = Number(arguments[1]);
            if (i != i) {
                i = 0;
            } else if (i != 0 && i != Infinity && i != -Infinity) {
                i = (i > 0 || -1) * Math.floor(Math.abs(i));
            }
        }
        if (i >= e) {
            return -1;
        }
        var n = i >= 0 ? i : Math.max(e - Math.abs(i), 0);
        for (;n < e; n++) {
            if (n in o && o[n] === t) {
                return n;
            }
        }
        return -1;
    };
}

String.prototype.format = function() {
    var t = this;
    for (var o = 0; o < arguments.length; o++) {
        var e = new RegExp("\\{" + o + "\\}", "gm");
        t = t.replace(e, arguments[o]);
    }
    return t;
};

function HtmlTemplate(t, o) {
    for (var e in o) {
        if (!o.hasOwnProperty(e)) {
            continue;
        }
        t = t.replace(new RegExp("{{" + e + "}}", "g"), o[e]);
    }
    return t;
}

