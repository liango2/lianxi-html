function DialogHelper() {}

DialogHelper.prototype.Init = function(t, o) {
    this.selectors = {
        dialog: "#" + t
    };
    var e = {
        resizable: false,
        height: 200,
        width: 400,
        modal: true
    };
    for (var i in o) {
        e[i] = o[i];
    }
    if (typeof o.buttonSet == "string") {
        switch (o.buttonSet) {
          case "ok-cancel":
            e.buttons = {
                Cancel: function() {
                    ui$(this).dialog("close");
                },
                OK: function() {
                    o.onOk();
                }
            };
            break;

          default:
            alert('"{0}" is not a valid button set'.format(o.buttonSet));
            break;
        }
    }
    ui$(this.selectors.dialog).dialog(e);
};

DialogHelper.prototype.Open = function() {
    ui$(this.selectors.dialog).dialog("open");
};

DialogHelper.prototype.Close = function() {
    ui$(this.selectors.dialog).dialog("close");
};

