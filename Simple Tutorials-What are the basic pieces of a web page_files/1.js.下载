(function() {
    var t = this;
    var o;
    if (typeof t.require == "function") {
        try {
            var e = t.require("crypto").randomBytes;
            o = e && function() {
                return e(16);
            };
        } catch (i) {}
    }
    if (!o && t.crypto && crypto.getRandomValues) {
        var n = new Uint8Array(16);
        o = function x() {
            crypto.getRandomValues(n);
            return n;
        };
    }
    if (!o) {
        var a = new Array(16);
        o = function() {
            for (var t = 0, o; t < 16; t++) {
                if ((t & 3) === 0) o = Math.random() * 4294967296;
                a[t] = o >>> ((t & 3) << 3) & 255;
            }
            return a;
        };
    }
    var l = typeof t.Buffer == "function" ? t.Buffer : Array;
    var d = [];
    var s = {};
    for (var r = 0; r < 256; r++) {
