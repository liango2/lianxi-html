function getUrlParameterByName(t) {
    t = t.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
    var o = "[\\?&]" + t + "=([^&#]*)";
    var e = new RegExp(o);
    var i = e.exec(window.location.search);
    if (i == null) return null; else return decodeURIComponent(i[1].replace(/\+/g, " "));
}

function GetLoggedInUser() {
    return $("#site-data").attr("data-user-id");
}

function IsProd() {
    return $("#site-data").attr("data-is-prod").toLowerCase() === "true";
}

function IsModerator() {
    return $("#site-data").attr("data-is-mod").toLowerCase() === "true";
}

function SiteUrl(t) {
    return IsProd() ? "https://simpletutorials.com" + t : t;
}

$(window).load(function() {});

function SendRequest(t, o, e, i, n, a) {
    var l = null;
    if (n && n.loadingSpinContainerId) {
        l = new LoadingSpinImage();
        l.Show(n.loadingSpinContainerId);
    }
    if (!n || typeof n.hideMask == "undefined" && typeof n.loadingSpinContainerId == "undefined") {
        i = typeof i == "undefined" ? 0 : i;
        switch (i) {
          case 0:
            $("#bodytd").mask("Saving...");
            break;

          case 1:
            $("#bodytd").mask("Loading...");
            break;

          case 2:
            $("#bodytd").mask("Deleting...");
            break;
        }
    }
    if (ST.isDebug && console && console.log) {
        console.log(o);
        if (o.jsonContent) {
            console.log(JSON.parse(o.jsonContent));
        }
    }
    var d = {
        contentType: "application/json",
        dataType: a ? "jsonp" : "json",
        data: JSON.stringify(o),
        url: t,
        error: function() {
            $("#bodytd").unmask();
            if (l) {
                l.Hide();
            }
            alert("Sorry, a system error has occurred. Please try again later.");
        },
        success: function(t) {
            $("#bodytd").unmask();
            if (l) {
                l.Hide();
            }
            if (ST.isDebug && console && console.log) {
                console.log(t);
            }
            e(t);
        },
        type: "POST"
    };
    if (n) {
        for (var s in n) {
            if (n.hasOwnProperty(s)) {
                d[s] = n[s];
            }
        }
    }
    $.ajax(d);
}

function PasswordMeetsRequirements(t) {
    if (!t) {
        alert("The password cannot be blank");
        return false;
    }
    if (t.length < 8) {
        alert("The password must be at least 8 characters");
        return false;
    }
    var o = 0;
    for (var e = 0; e < t.length; e++) {
        if (!/^[a-z0-9]+$/i.test(t[e])) {
            o++;
        }
    }
    if (o < 1) {
        alert("The password must contain at least one non-alphanumeric character (e.g. #$%^&*.)");
        return false;
    }
    return true;
}

function LoadingSpinImage() {}

LoadingSpinImage.prototype.Show = function(t) {
    this.containerId = t;
    $("#" + t).html('<div class="loadingSpin"></div>');
};

LoadingSpinImage.prototype.Hide = function() {
    $("#" + this.containerId).html("");
};

function escapeHTML(t) {
    var o = document.createElement("pre");
    var e = document.createTextNode(t);
    o.appendChild(e);
    return o.innerHTML;
}

function generateGuid() {
    return uuid.v4();
}

function getParameterUrl(t) {
    var o = window.location.search.substring(1), e, i, n = o.split("&");
    for (e = 0; e < n.length; e++) {
        i = n[e].split("=");
        if (i[0] == t) {
            return decodeURIComponent(i[1]);
        }
    }
    return null;
}

(function() {
    var t = {};
