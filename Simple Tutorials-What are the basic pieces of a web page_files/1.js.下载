        s[d[r]] = r;
    }
    function p(t, o, e) {
        var i = o && e || 0, n = 0;
        o = o || [];
        t.toLowerCase().replace(/[0-9a-f]{2}/g, function(t) {
            if (n < 16) {
                o[i + n++] = s[t];
            }
        });
        while (n < 16) {
            o[i + n++] = 0;
        }
        return o;
    }
    function u(t, o) {
        var e = o || 0, i = d;
        return i[t[e++]] + i[t[e++]] + i[t[e++]] + i[t[e++]] + "-" + i[t[e++]] + i[t[e++]] + "-" + i[t[e++]] + i[t[e++]] + "-" + i[t[e++]] + i[t[e++]] + "-" + i[t[e++]] + i[t[e++]] + i[t[e++]] + i[t[e++]] + i[t[e++]] + i[t[e++]];
    }
    var c = o();
    var v = [ c[0] | 1, c[1], c[2], c[3], c[4], c[5] ];
    var m = (c[6] << 8 | c[7]) & 16383;
    var b = 0, g = 0;
    function y(t, o, e) {
        var i = o && e || 0;
        var n = o || [];
        t = t || {};
        var a = t.clockseq != null ? t.clockseq : m;
        var l = t.msecs != null ? t.msecs : new Date().getTime();
        var d = t.nsecs != null ? t.nsecs : g + 1;
        var s = l - b + (d - g) / 1e4;
        if (s < 0 && t.clockseq == null) {
            a = a + 1 & 16383;
        }
        if ((s < 0 || l > b) && t.nsecs == null) {
            d = 0;
        }
        if (d >= 1e4) {
            throw new Error("uuid.v1(): Can't create more than 10M uuids/sec");
        }
        b = l;
        g = d;
        m = a;
        l += 122192928e5;
        var r = ((l & 268435455) * 1e4 + d) % 4294967296;
